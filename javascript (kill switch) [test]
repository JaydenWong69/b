
javascript:(function() {
  var defaultRedirectUrl = "google.com"; // Replace with the default URL
  var customRedirectUrl = prompt("Enter the redirect URL:", defaultRedirectUrl);
  
  if (customRedirectUrl === null) {
    return; // User canceled the prompt
  }
  
  var confirmMessage = "Are you sure you want to activate the kill switch?";
  var confirmationResult = confirm(confirmMessage);
  
  if (confirmationResult) {
    window.location.href = customRedirectUrl;
  }
})();



//ver 1.0
